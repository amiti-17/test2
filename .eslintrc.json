{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": "airbnb-base",
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "object-curly-newline" : "warn",
        "no-console": "off",
        "no-alert": "off",
        "no-plusplus": "warn",
        "max-classes-per-file": "warn",
        "no-unused-vars": "warn",
        "max-len": "warn"
    }
}
