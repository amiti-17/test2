{
    "env": {
        "es2021": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "no-console": "off",
        "no-alert": "off",
        "no-plusplus": "off",
        //"no-else-return": "off",
        "max-classes-per-file": "off",
        "no-unused-vars": "off"
    }
}
